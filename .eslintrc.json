{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "indent": ["error", "tab"],
      "semi": ["error", "always"],
      "object-curly-spacing": ["error", "always"],
      "quotes": ["error", "single"],
      "max-len": ["error", { "code": 90 }],
      "object-shorthand": ["error", "never"],
      "camelcase": "off"
    }
}
